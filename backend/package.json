{
  "name": "grosz-do-grosza-api-backend-server",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "description": "Node backend for proxying OpenAI API requests",
  "main": "server.js",
  "scripts": {
    "check-start": "node --env-file=.env.local check-start.js",
    "start:dev": "npm run check-start && nodemon --env-file=.env.local server.js",
    "start:prod": "npm run check-start && node --env-file=.env.local server.js",
    "dev": "npm run start:dev",
    "start": "npm run start:prod"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "nodemon": "^3.1.11"
  }
}
